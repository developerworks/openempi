        <xb:package>org.openhie.openempi.configuration.xml</xb:package>
    </xb:namespace>
    <xb:namespace uri="http://configuration.openempi.openhie.org/mpicomponent">
            <xb:package>org.openhie.openempi.configuration.xml.mpicomponent</xb:package>
    </xb:namespace>
    <xb:namespace uri="http://configuration.openempi.openhie.org/basic-blocking">
            <xb:package>org.openhie.openempi.configuration.xml.basicblocking</xb:package>
    </xb:namespace>
    <xb:namespace uri="http://configuration.openempi.openhie.org/basic-blocking-hp">
            <xb:package>org.openhie.openempi.configuration.xml.basicblockinghp</xb:package>
    </xb:namespace>
    <xb:namespace uri="http://configuration.openempi.openhie.org/sorted-neighborhood-blocking">
            <xb:package>org.openhie.openempi.configuration.xml.sortedneighborhoodblocking</xb:package>
    </xb:namespace>
    <xb:namespace uri="http://configuration.openempi.openhie.org/suffix-array-blocking">
